{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ES2020",
    "types": ["@cloudflare/workers-types"],
    "noEmit": true,
    "strict": true,
    "paths": {
      "@/*": ["./*", "./src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}

