{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ES2020",
    "types": [
      "@cloudflare/workers-types"
    ],
    "noEmit": true,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}